# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'JLKit' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for JLKit
  pod 'AFNetworking'
  pod 'Masonry'
  pod 'MJRefresh'
  pod 'BlocksKit'
  pod 'Typeset'
  pod 'SDWebImage'
  pod 'FLAnimatedImage'
  pod 'MJExtension'
  pod 'YYModel'
  pod 'JSONModel'
  # 瀑布流
  pod 'CHTCollectionViewWaterfallLayout'
  # 从左开始不规则布局，适用于搜索历史和标签选择之类
  pod 'UICollectionViewLeftAlignedLayout'

end
